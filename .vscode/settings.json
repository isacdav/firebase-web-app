{
  "workbench.colorCustomizations": {
    "activityBar.foreground": "#111111",
    "activityBarBadge.background": "#010101",
    "activityBarBadge.foreground": "#F0F0F0",
    "statusBar.foreground": "#F0F0F0",
    "titleBar.activeBackground": "#FACC15",
    "titleBar.activeForeground": "#010101",
    "titleBar.inactiveBackground": "#FACC15",
    "titleBar.inactiveForeground": "#555555"
  },
  "eslint.format.enable": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit",
    "source.sortMembers": "explicit"
  }
}
